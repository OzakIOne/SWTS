{
  "name": "tanstack-start-example-basic",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "lint": "oxlint .",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "db": "drizzle-kit",
    "auth:secret": "pnpm dlx @better-auth/cli@latest secret",
    "auth:generate": "pnpm dlx @better-auth/cli@latest generate --config ./src/lib/auth/index.ts --y --output ./src/lib/db/schema/auth.schema.ts && prettier --write ./src/lib/db/schema/auth.schema.ts"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-form": "^1.14.1",
    "@tanstack/react-pacer": "^0.11.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/react-router": "^1.128.0",
    "@tanstack/react-router-devtools": "^1.128.0",
    "@tanstack/react-router-with-query": "^1.128.0",
    "@tanstack/react-start": "^1.128.1",
    "@tanstack/react-virtual": "^3.13.12",
    "better-auth": "^1.3.4",
    "daisyui": "^5.0.46",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "postgres": "^3.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@tanstack/eslint-plugin-router": "^1.129.7",
    "@types/node": "^24.0.15",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.1.11",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
